{
  "name": "vscode-ai-fixer",
  "displayName": "vsCodeAiFixer",
  "description": "AI-powered ESLint and TypeScript error fixer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-ai-fixer.fixAllErrors",
        "title": "AI Fix: Fix All ESLint and TypeScript Errors"
      },
      {
        "command": "vscode-ai-fixer.fixCurrentFile",
        "title": "AI Fix: Fix Current File Errors"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-ai-fixer.fixCurrentFile",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f",
        "when": "editorHasMultipleSelections"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.30",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  }
}
